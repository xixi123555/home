.wapre
    display flex
    justify-content center
    flex-direction column
    .text
        width 100px
        margin-top 20px
        font-weight 1000
        color #fff
    .link
        margin-right 100px
        width 100px
        height 100px
        .imgLink
            width 100%
            height auto
            cursor pointer
            transition all 0.6s
            &:hover
                transform scale(1.3)
